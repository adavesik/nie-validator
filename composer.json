{
  "name": "sevada/nie-validator",
  "description": "Laravel validation rule for Spanish NIE numbers",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=8.0",
    "illuminate/support": "^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "orchestra/testbench": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Sevada\\NieValidator\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Sevada\\NieValidator\\NieValidatorServiceProvider"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sevada\\NieValidator\\Tests\\": "tests/"
    }
  }
}