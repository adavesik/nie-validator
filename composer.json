{
  "name": "sevada/nif-validator",
  "description": "Laravel validation rules for Spanish NIE and DNI numbers",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=8.0",
    "illuminate/support": "^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "orchestra/testbench": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Sevada\\NifValidator\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Sevada\\NifValidator\\NifValidatorServiceProvider"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sevada\\NifValidator\\Tests\\": "tests/"
    }
  }
}